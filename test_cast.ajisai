# 型変換ワード群のテストケース（STR/NUM/BOOL/NIL/VEC）

# ============================================================================
# STR - 任意の型 → String
# ============================================================================

# 数値 → 文字列
[ 42 ] STR

# 分数 → 文字列
[ 1 ] [ 3 ] / STR

# Boolean → 文字列
[ TRUE ] STR
[ FALSE ] STR

# Nil → 文字列
[ nil ] STR

# ベクトル → 文字列（スペース区切り）
[ [ 1 2 3 ] ] STR
[ [ TRUE FALSE nil ] ] STR

# ============================================================================
# NUM - String → Number
# ============================================================================

# 整数文字列 → 数値
[ '42' ] NUM

# 分数文字列 → 数値
[ '1/3' ] NUM

# 小数文字列 → 数値（分数に変換）
[ '3.14' ] NUM

# 科学的記数法
[ '1.5e2' ] NUM

# ============================================================================
# BOOL - String → Boolean
# ============================================================================

# TRUE/FALSE（大文字）
[ 'TRUE' ] BOOL
[ 'FALSE' ] BOOL

# true/false（小文字）
[ 'true' ] BOOL
[ 'false' ] BOOL

# 混在（大文字小文字無視）
[ 'True' ] BOOL
[ 'False' ] BOOL

# ============================================================================
# NIL - String → Nil
# ============================================================================

# nil（小文字）
[ 'nil' ] NIL

# NIL（大文字）
[ 'NIL' ] NIL

# Nil（混在）
[ 'Nil' ] NIL

# ============================================================================
# VEC - String → Vector（要素は全て文字列）
# ============================================================================

# 数値のような文字列
[ '1 2 3' ] VEC

# 単語
[ 'hello world' ] VEC

# 混在
[ '42 TRUE nil hello' ] VEC

# 空文字列
[ '' ] VEC

# スペースのみ
[ '   ' ] VEC

# 連続スペース
[ 'a  b  c' ] VEC

# ============================================================================
# 組み合わせ例
# ============================================================================

# ベクトル → 文字列 → ベクトル（文字列要素）
[ [ 1 2 3 ] ] STR VEC

# 数値 → 文字列 → 数値
[ 42 ] STR NUM

# Boolean → 文字列 → Boolean
[ TRUE ] STR BOOL

# 文字列から数値に変換してから文字列化
[ '123' ] NUM STR

# 複数の要素を個別に変換
[ '1' '2' '3' ] VEC
[ '1' ] NUM
[ '2' ] NUM
[ '3' ] NUM

# 文字列連結の例
[ 10 ] [ 20 ] + STR [ ' is the sum' ] CONCAT
