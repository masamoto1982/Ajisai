# CAST ワードのテストケース

# ============================================================================
# Vector → String
# ============================================================================

# 数値ベクトルを文字列化
[ 1 2 3 ] CAST

# Boolean ベクトルを文字列化
[ TRUE FALSE ] CAST

# 混合型ベクトルを文字列化
[ 1 nil TRUE 'hello' ] CAST

# ============================================================================
# String → Vector（型推論）
# ============================================================================

# 数値文字列をベクトルに
[ '1 2 3' ] CAST

# Boolean文字列をベクトルに
[ 'TRUE FALSE' ] CAST

# nil を含む文字列
[ '1 nil 3' ] CAST

# 混合型文字列
[ '42 TRUE nil hello' ] CAST

# 小文字の TRUE/FALSE/nil
[ 'true false nil' ] CAST

# ============================================================================
# 各型 → String
# ============================================================================

# Number → String
[ 42 ] CAST

# 分数 → String
[ 1 ] [ 3 ] / CAST

# Boolean → String
[ TRUE ] CAST
[ FALSE ] CAST

# Nil → String
[ nil ] CAST

# ============================================================================
# 往復変換（可逆性テスト）
# ============================================================================

# Vector → String → Vector
[ 1 2 3 ] CAST CAST

# Number → String → Vector
[ 42 ] CAST CAST

# Boolean → String → Vector
[ TRUE ] CAST CAST

# ============================================================================
# 文字列連結との組み合わせ
# ============================================================================

# 数値を文字列化してから連結
[ 1 2 3 ] CAST [ ' は数列です' ] CONCAT

# 計算結果を文字列として連結
[ 10 ] [ 20 ] + CAST [ ' is the sum' ] CONCAT

# ============================================================================
# エッジケース
# ============================================================================

# 空文字列
[ '' ] CAST

# スペースのみの文字列
[ '   ' ] CAST

# 科学的記数法
[ '1.5e2' ] CAST
