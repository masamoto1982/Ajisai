# MOD テスト
[ 7 ] [ 3 ] MOD PRINT      # → [ 1 ]
[ 10 ] [ 4 ] MOD PRINT     # → [ 2 ]
[ 7 8 9 ] [ 3 ] MOD PRINT  # → [ 1 2 0 ]
[ -7 ] [ 3 ] MOD PRINT     # → [ 2 ] (数学的剰余)

# FLOOR テスト
[ 7/3 ] FLOOR PRINT        # → [ 2 ]
[ -7/3 ] FLOOR PRINT       # → [ -3 ]
[ 5 ] FLOOR PRINT          # → [ 5 ]

# CEIL テスト
[ 7/3 ] CEIL PRINT         # → [ 3 ]
[ -7/3 ] CEIL PRINT        # → [ -2 ]
[ 5 ] CEIL PRINT           # → [ 5 ]

# ROUND テスト
[ 5/2 ] ROUND PRINT        # → [ 3 ]
[ -5/2 ] ROUND PRINT       # → [ -3 ]
[ 7/3 ] ROUND PRINT        # → [ 2 ]
